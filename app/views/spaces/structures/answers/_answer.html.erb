<div id="<%= dom_id answer %>">
  <p>
    <strong>Structure:</strong>
    <%= answer.structure_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= answer.user_id %>
  </p>

  <p>
    <strong>Form Data:</strong>
    <%= answer.form_data %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= answer.status %>
  </p>

  <p>
    <strong>Stared:</strong>
    <%= answer.stared %>
  </p>

  <% if answer.file_data.present? %>
    <div class="mb-3">
      <p>
        <strong>File:</strong>
        <%= link_to answer.filename, download_space_structure_answer_path(answer.structure.space, answer.structure, answer) %>
      </p>
    </div>
  <% end %>

</div>